@model IEnumerable<KalingaCMSFinal.Models.ref_Barangay>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.countryID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.regionID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.provinceID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.municipalityID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Barangay)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.countryID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.regionID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.provinceID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.municipalityID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Barangay)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.barangayID }) |
                @Html.ActionLink("Details", "Details", new { id=item.barangayID }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.barangayID })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
