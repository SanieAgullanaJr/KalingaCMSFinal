        <td>
            @Html.DisplayFor(modelItem => item.MunicipalityID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MigratingIN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MigratingINPer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MigratingOUT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MigratingOUTPer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.YearTaken)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PopMigrationID }) |
            @Html.ActionLink("Details", "Details", new { id=item.PopMigrationID }) |
