@model IEnumerable<KalingaCMSFinal.Models.PopulationMigrationRate>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MunicipalityID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MigratingIN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MigratingINPer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MigratingOUT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MigratingOUTPer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.YearTaken)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
